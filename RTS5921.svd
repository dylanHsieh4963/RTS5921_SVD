<?xml version="1.0" encoding="utf-8"?>
<device schemaVersion="1.3" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD.xsd">
  <!-- Product & Vendor -->
  <vendor>Realtek Semiconductor Corp.</vendor>
  <vendorID>Realtek SIBG</vendorID>
  <name>RTS5921</name>
  <series>RTS5921 Series</series>
  <version>0.01</version>
  <description>Realtek Real-M300 Arm v8-M Instruction Processor</description>
  <licenseText>
	  The license belong to Realtek Semiconductor Corp. All the rights reserved.
  </licenseText>

  <cpu>
    <name>ARMV8MBL</name>
    <revision>r0p0</revision>
    <endian>little</endian>
    <mpuPresent>true</mpuPresent>
    <fpuPresent>false</fpuPresent>
    <vtorPresent>true</vtorPresent>
    <nvicPrioBits>3</nvicPrioBits>
    <vendorSystickConfig>false</vendorSystickConfig>
  </cpu>

  <!-- byte addressable memory -->
  <addressUnitBits>8</addressUnitBits>
  <!-- bus width is 32 bits -->
  <width>32</width>
  <!-- this is the default size (number of bits) of all peripherals and register that do not define "size" themselves -->
  <size>32</size>
  <!-- default access permission for all subsequent registers -->
  <access>read-write</access>
  <!-- by default all bits of the registers are initialized to 0 on reset -->
  <resetValue>0x00000000</resetValue>
  <!-- by default all 32Bits of the registers are used -->
  <resetMask>0xffffffff</resetMask>

  <!-- Peripherals -->
  <peripherals>
    <!-- INTR_SPIC -->
    <peripheral>
      <!-- Description -->
      <name>INTR_SPIC</name>
      <version>1.00</version>
      <description>Realtek RXI-312 SPI Controller</description>
      <groupName>SPIC</groupName>
      <headerStructName>SPIC</headerStructName>
      <baseAddress>0x40000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <!-- Address Range -->
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <!-- Interrup -->
      <interrupt>
        <name>INTR_SPIC</name>
        <description>Internal SPIC Interrupt</description>
        <value>9</value>
      </interrupt>

      <!-- Registers -->
      <registers>
				<!-- CTRLR0 -->
				<register>
						<name>CTRLR0</name>
						<description>Control Register 0</description>
						<addressOffset>0x000</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0xFFFFFFFF</resetValue>
            <fields>
                <field>
                    <name>USER_MODE</name>
                    <description>
                        User mode bit.
                    </description>
                    <bitRange>[31:31]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>UAR</name>
                    <description>
                        User mode bit auto reset. Enable to auto reset USER_MODE to 0 after user mode transaction is over.
                    </description>
                    <bitRange>[30:30]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>CK_MTIMES</name>
                    <description>Indicate the auto-check times</description>
                    <bitRange>[27:23]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>FAST_RD</name>
                    <description>
                        Enable use of fast read command in user mode. Use FBAUDR as spi_sclk when set.
                    </description>
                    <bitRange>[22:22]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>CMD_CH</name>
                    <description>
                        Channel number of command phase. 0: single, 1: dual, 2: quad, 3: octal.
                    </description>
                    <bitRange>[21:20]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>DATA_CH</name>
                    <description>
                        Channel number of data phase. 0: single, 1: dual, 2: quad, 3: octal.
                    </description>
                    <bitRange>[19:18]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>ADDR_CH</name>
                    <description>
                        Channel number for address phase. 0: single, 1: dual, 2: quad, 3: octal.
                    </description>
                    <bitRange>[17:16]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>DDR_EN_CMD_CH</name>
                    <description>Enable DDR mode on CMD_CH (2-Byte CMD type). Only if DDR_EN defined.</description>
                    <bitRange>[15:15]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>DDR_EN_DATA_CH</name>
                    <description>Enable DDR mode on DATA_CH. Only if DDR_EN defined.</description>
                    <bitRange>[14:14]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>DDR_EN_ADDR_CH</name>
                    <description>Enable DDR mode on ADDR_CH. Only if DDR_EN defined.</description>
                    <bitRange>[13:13]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>GCLK_DIS</name>
                    <description>Disable gated clock of icg cell.</description>
                    <bitRange>[10:10]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>TMOD</name>
                    <description>
                        Transfer mode. 00: transmit mode, others: receive mode.
                    </description>
                    <bitRange>[9:8]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>SCPOL</name>
                    <description>Serial clock polarity. 0: inactive state is low, 1: inactive state is high.</description>
                    <bitRange>[7:7]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>SCPH</name>
                    <description>Serial clock phase. 0: toggles in middle of 1st data bit, 1: toggles at start of 1st data bit.</description>
                    <bitRange>[6:6]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>SIPOL_EN</name>
                    <description>Enable SIPOL control for SPI in/out signal polarity.</description>
                    <bitRange>[5:5]</bitRange>
                    <access>read-write</access>
                </field>
                <field>
                    <name>SIPOL</name>
                    <description>SPI input signal polarity</description>
                    <bitRange>[4:0]</bitRange>
                    <access>read-write</access>
                </field>
            </fields>
				</register>

				<!-- RX_NDF -->
				<register>
						<name>RX_NDF</name>
						<description>Control Register 1</description>
						<addressOffset>0x004</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x00000000</resetValue>
            <fields>
              <!-- NDF Field -->
              <field>
                <name>NDF</name>
                <description>Indicates the number of data frames</description>
                <bitRange>[24:0]</bitRange>
                <access>read-write</access>
                <description>
                  Indicate a number of data frames. If Data-Split-Read function enables, NDF can be set larger than FIFO size, with a maximum size of 65535 bytes.
                </description>
              </field>
            </fields>
				</register>

				<!-- SSIENR -->
				<register>
					<name>SSIENR</name>
					<description>SPIC Enable Register</description>
					<addressOffset>0x008</addressOffset>
					<size>32</size>
					<access>read-write</access>
					<resetValue>0x00000000</resetValue>
          <fields>
              <field>
                  <name>CUR_GP</name>
                  <description>Current group, for frequency change .</description>
                  <bitRange>[11:10]</bitRange>
                  <access>read-only</access>
              </field>
              <field>
                  <name>GP_NUM</name>
                  <description>Target group, for frequency change.</description>
                  <bitRange>[9:8]</bitRange>
                  <access>read-write</access>
              </field>
              <field>
                  <name>PGM_RST_TEST_EN</name>
                  <description>Pulse warm reset. Test only.</description>
                  <bitRange>[4:4]</bitRange>
                  <access>write-only</access>
              </field>
              <field>
                  <name>FRQC</name>
                  <description>Enable/complete frequency change.</description>
                  <bitRange>[2:2]</bitRange>
                  <access>read-write</access>
              </field>
              <field>
                  <name>ATCK_CMD</name>
                  <description>Enable auto check command.</description>
                  <bitRange>[1:1]</bitRange>
                  <access>read-write</access>
              </field>
              <field>
                  <name>SPIC_EN</name>
                  <description>Enable SPIC / user mode transaction.</description>
                  <bitRange>[0:0]</bitRange>
                  <access>read-write</access>
              </field>
          </fields>
				</register>

				<!-- SER -->
				<register>
						<name>SER</name>
						<description>Slave Enable Register</description>
						<addressOffset>0x010</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x00000000</resetValue>
				</register>

				<!-- BAUDR -->
				<register>
					<name>BAUDR</name>
					<description>Baud Rate Select</description>
					<addressOffset>0x014</addressOffset>
					<size>32</size>
					<access>read-write</access>
					<resetValue>0xFFFFFFFF</resetValue>
          <fields>
            <!-- SCKDV Field -->
            <field>
                <name>SCKDV</name>
                <description>
                  Define spi_sclk divider value. The frequency of spi_sclk is derived from:
                  Frequency of spi_sclk = Frequency of bus_clk / (2*SCKDV).
                </description>
                <bitRange>[11:0]</bitRange>
                <access>read-write</access>
              </field>
            </fields>
	      </register>

				<!-- TXFTLR -->
				<register>
						<name>TXFTLR</name>
						<description>Transmit FIFO Threshold level</description>
						<addressOffset>0x018</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x00000000</resetValue>
            <fields>
              <!-- TFT Field -->
              <field>
                <name>TFT</name>
                <description>Transmit FIFO threshold.</description>
                <bitRange>[4:0]</bitRange>
                <access>read-write</access>
              </field>
            </fields>
				</register>

				<!-- RXFTLR -->
				<register>
						<name>RXFTLR</name>
						<description>Receive FIFO Threshold level</description>
						<addressOffset>0x01C</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0xFFFFFFFF</resetValue>
            <fields>
              <!-- RFT Field -->
              <field>
                <name>RFT</name>
                <description>Receive FIFO threshold.</description>
                <bitRange>[4:0]</bitRange> <!-- FIFO_ABW = 5, so range is 0 to 4 -->
                <access>read-write</access>
              </field>
            </fields>
				</register>

				<!-- TXFLR -->
				<register>
						<name>TXFLR</name>
						<description>Transmit FIFO level Register</description>
						<addressOffset>0x020</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x00000000</resetValue>
            <fields>
              <!-- TXFLR Field -->
              <field>
                <name>TXFLR</name>
                <description>Indicates the FIFO level of valid data during transmission.</description>
                <bitRange>[5:0]</bitRange>
                <access>read-only</access>
              </field>
            </fields>
				</register>

				<!-- RXFLR -->
				<register>
						<name>RXFLR</name>
						<description>Receive FIFO level Register</description>
						<addressOffset>0x024</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x00000000</resetValue>
            <fields>
              <!-- RXFLR Field -->
              <field>
                <name>RXFLR</name>
                <description>Indicates the FIFO level of valid data during reception.</description>
                <bitRange>[5:0]</bitRange> <!-- FIFO_ABW is 5; thus, bits 0 to 5 are used -->
                <access>read-only</access>
              </field>
            </fields>
				</register>

				<!-- SR -->
				<register>
						<name>SR</name>
						<description>Status Register</description>
						<addressOffset>0x028</addressOffset>
						<size>32</size>
						<access>read-only</access>
						<resetValue>0x00000006</resetValue>
    
            <fields>
              <!-- ATWR_RDSR_N Field -->
              <field>
                <name>ATWR_RDSR_N</name>
                <description>The previous auto write cmd didn't check the status register (RDSR).</description>
                <bitRange>[8:8]</bitRange>
                <access>read-only</access>
              </field>
              <!-- BOOT_FIN Field -->
              <field>
                <name>BOOT_FIN</name>
                <description>Boot Finish status indicator.</description>
                <bitRange>[7:7]</bitRange>
                <access>read-only</access>
              </field>

              <!-- DCOL Field -->
              <field>
                <name>DCOL</name>
                <description>Data collision or transmitting status.</description>
                <bitRange>[6:6]</bitRange>
                <access>read-only</access>
              </field>

              <!-- TXE Field -->
              <field>
                <name>TXE</name>
                <description>Transmission error indicator.</description>
                <bitRange>[5:5]</bitRange>
                <access>read-only</access>
              </field>

              <!-- RFF Field -->
              <field>
                <name>RFF</name>
                <description>Receive FIFO full status.</description>
                <bitRange>[4:4]</bitRange>
                <access>read-only</access>
              </field>

              <!-- RFNE Field -->
              <field>
                <name>RFNE</name>
                <description>Receive FIFO not empty status.</description>
                <bitRange>[3:3]</bitRange>
                <access>read-only</access>
              </field>

              <!-- TFE Field -->
              <field>
                <name>TFE</name>
                <description>Transmit FIFO empty status.</description>
                <bitRange>[2:2]</bitRange>
                <access>read-only</access>
              </field>

              <!-- TFNF Field -->
              <field>
                <name>TFNF</name>
                <description>Transmit FIFO not full status.</description>
                <bitRange>[1:1]</bitRange>
                <access>read-only</access>
              </field>

              <!-- BUSY Field -->
              <field>
                <name>BUSY</name>
                <description>SPIC busy flag.</description>
                <bitRange>[0:0]</bitRange>
                <access>read-only</access>
              </field>
            </fields>
				</register>

				<!-- IMR -->
				<register>
						<name>IMR</name>
						<description>Interrupt Mask Register</description>
						<addressOffset>0x02C</addressOffset>
						<size>32</size>
						<access>read-write</access>
						<resetValue>0x000001EE</resetValue>

            <fields>
              <!-- ACSIM Field -->
              <field>
                <name>ACSIM</name>
                <description>Auto-check Flash Status raw interrupt mask.</description>
                <bitRange>[11:11]</bitRange>
                <access>read-write</access>
              </field>

              <!-- RXSIM Field -->
              <field>
                <name>RXSIM</name>
                <description>Transmit split interrupt mask.</description>
                <bitRange>[10:10]</bitRange>
                <access>read-write</access>
              </field>

              <!-- TXSIM Field -->
              <field>
                <name>TXSIM</name>
                <description>Transmit split interrupt mask.</description>
                <bitRange>[9:9]</bitRange>
                <access>read-write</access>
              </field>

              <!-- ACEIM Field -->
              <field>
                <name>ACEIM</name>
                <description>Auto-check timeout error interrupt mask.</description>
                <bitRange>[8:8]</bitRange>
                <access>read-write</access>
              </field>

              <!-- BYEIM Field -->
              <field>
                <name>BYEIM</name>
                <description>Byte-Enable error interrupt mask.</description>
                <bitRange>[7:7]</bitRange>
                <access>read-write</access>
              </field>

              <!-- WBEIM Field -->
              <field>
                <name>WBEIM</name>
                <description>Write burst error interrupt mask.</description>
                <bitRange>[6:6]</bitRange>
                <access>read-write</access>
              </field>

              <!-- FSEIM Field -->
              <field>
                <name>FSEIM</name>
                <description>FIFO size error interrupt mask.</description>
                <bitRange>[5:5]</bitRange>
                <access>read-write</access>
              </field>

              <!-- RXFIM Field -->
              <field>
                <name>RXFIM</name>
                <description>Receive FIFO full interrupt mask.</description>
                <bitRange>[4:4]</bitRange>
                <access>read-write</access>
              </field>

              <!-- RXOIM Field -->
              <field>
                <name>RXOIM</name>
                <description>Receive FIFO overflow interrupt mask.</description>
                <bitRange>[3:3]</bitRange>
                <access>read-write</access>
              </field>

              <!-- RXUIM Field -->
              <field>
                <name>RXUIM</name>
                <description>Receive FIFO underflow interrupt mask.</description>
                <bitRange>[2:2]</bitRange>
                <access>read-write</access>
              </field>

              <!-- TXOIM Field -->
              <field>
                <name>TXOIM</name>
                <description>Transmit FIFO overflow interrupt mask.</description>
                <bitRange>[1:1]</bitRange>
                <access>read-write</access>
              </field>

              <!-- TXEIM Field -->
              <field>
                <name>TXEIM</name>
                <description>Transmit FIFO empty interrupt mask.</description>
                <bitRange>[0:0]</bitRange>
                <access>read-write</access>
              </field>
            </fields>
				</register>

				<!-- ISR -->
				<register>
						<name>ISR</name>
						<description>Interrupt Status Register</description>
						<addressOffset>0x030</addressOffset>
						<size>32</size>
						<access>read-only</access>
						<resetValue>0x00000000</resetValue>

         <fields>
            <!-- ACSIS Field -->
            <field>
               <name>ACSIS</name>
               <description>Auto-check Flash Status after masking. 1: spi_aceir_r is active after masking. 0: spi_aceir_r is not active after masking.</description>
               <bitRange>[11:11]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXSIS Field -->
            <field>
               <name>RXSIS</name>
               <description>Receive split interrupt status after masking. 1: spi_rxsir_r is active after masking. 0: spi_rxsir_r is not active after masking.</description>
               <bitRange>[10:10]</bitRange>
               <access>read-only</access>
            </field>

            <!-- TXSIS Field -->
            <field>
               <name>TXSIS</name>
               <description>Transmit split interrupt status after masking. 1: spi_txsir_r is active after masking. 0: spi_txsir_r is not active after masking.</description>
               <bitRange>[9:9]</bitRange>
               <access>read-only</access>
            </field>

            <!-- ACEIS Field -->
            <field>
               <name>ACEIS</name>
               <description>Auto-check timeout error status after masking. 1: spi_aceir_r is active after masking. 0: spi_aceir_r is not active after masking.</description>
               <bitRange>[8:8]</bitRange>
               <access>read-only</access>
            </field>

            <!-- BYEIS Field -->
            <field>
               <name>BYEIS</name>
               <description>Byte-Enable error interrupts status after masking. 1: spi_byeir_r is active after masking. 0: spi_byeir_r is not active after masking.</description>
               <bitRange>[7:7]</bitRange>
               <access>read-only</access>
            </field>

            <!-- WBEIS Field -->
            <field>
               <name>WBEIS</name>
               <description>Write burst error interrupt status after masking. 1: spi_wbeir_r is active after masking. 0: spi_wbeir_r is not active after masking.</description>
               <bitRange>[6:6]</bitRange>
               <access>read-only</access>
            </field>

            <!-- FSEIS Field -->
            <field>
               <name>FSEIS</name>
               <description>FIFO size error interrupts status after masking. 1: spi_fesir_r is active after masking. 0: spi_fesir_r is not active after masking.</description>
               <bitRange>[5:5]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXFIS Field -->
            <field>
               <name>RXFIS</name>
               <description>Receive FIFO full interrupt status after masking. 1: spi_rxfir is active after masking. 0: spi_rxfir is not active after masking.</description>
               <bitRange>[4:4]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXOIS Field -->
            <field>
               <name>RXOIS</name>
               <description>Receive FIFO overflow interrupt status after masking. 1: spi_rxoir_r is active after masking. 0: spi_rxoir_r is not active after masking.</description>
               <bitRange>[3:3]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXUIS Field -->
            <field>
               <name>RXUIS</name>
               <description>Receive FIFO underflow interrupt status after masking. 1: spi_rxuir_r is active after masking. 0: spi_rxuir_r is not active after masking.</description>
               <bitRange>[2:2]</bitRange>
               <access>read-only</access>
            </field>

            <!-- TXOIS Field -->
            <field>
               <name>TXOIS</name>
               <description>Transmit FIFO overflow interrupt status after masking. 1: spi_txoir_r is active after masking. 0: spi_txoir_r is not active after masking.</description>
               <bitRange>[1:1]</bitRange>
               <access>read-only</access>
            </field>

            <!-- TXEIS Field -->
            <field>
               <name>TXEIS</name>
               <description>Transmit FIFO empty interrupt status after masking. 1: spi_txeir is active after masking. 0: spi_txeir is not active after masking.</description>
               <bitRange>[0:0]</bitRange>
               <access>read-only</access>
            </field>
         </fields>

				</register>

				<!-- RISR -->
				<register>
						<name>RISR</name>
						<description>Raw Interrupt Status Register</description>
						<addressOffset>0x034</addressOffset>
						<size>32</size>
						<access>read-only</access>
						<resetValue>0x00000000</resetValue>

         <fields>
            <!-- ACSIR Field -->
            <field>
               <name>ACSIR</name>
               <description>Auto-check Flash Status raw interrupt status prior to masking. 1: spi_acsir_r is active prior to masking. 0: spi_acsir_r is not active prior to masking.</description>
               <bitRange>[11:11]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXSIR Field -->
            <field>
               <name>RXSIR</name>
               <description>Receive split interrupt status prior to masking. 1: spi_rxsir_r is active prior to masking. 0: spi_rxsir_r is not active prior to masking.</description>
               <bitRange>[10:10]</bitRange>
               <access>read-only</access>
            </field>

            <!-- TXSIR Field -->
            <field>
               <name>TXSIR</name>
               <description>Transmit split interrupt status prior to masking. 1: spi_txsir_r is active prior to masking. 0: spi_txsir_r is not active prior to masking.</description>
               <bitRange>[9:9]</bitRange>
               <access>read-only</access>
            </field>

            <!-- ACEIR Field -->
            <field>
               <name>ACEIR</name>
               <description>Auto-check timeout error raw interrupt status prior to masking. 1: spi_aceir_r is active prior to masking. 0: spi_aceir_r is not active prior to masking.</description>
               <bitRange>[8:8]</bitRange>
               <access>read-only</access>
            </field>

            <!-- BYEIR Field -->
            <field>
               <name>BYEIR</name>
               <description>The Byte-Enable error interrupt raw status prior to masking. 1: spi_byeir_r is active prior to masking. 0: spi_byeir_r is not active prior to masking.</description>
               <bitRange>[7:7]</bitRange>
               <access>read-only</access>
            </field>

            <!-- WBEIR Field -->
            <field>
               <name>WBEIR</name>
               <description>Write burst error raw interrupt status prior to masking. 1: spi_wbeir_r is active prior to masking. 0: spi_wbeir_r is not active prior to masking.</description>
               <bitRange>[6:6]</bitRange>
               <access>read-only</access>
            </field>

            <!-- FSEIR Field -->
            <field>
               <name>FSEIR</name>
               <description>FIFO size error raw interrupt status prior to masking. 1: spi_fseir_r is active prior to masking. 0: spi_fseir_r is not active prior to masking.</description>
               <bitRange>[5:5]</bitRange>
               <access>read-only</access>
            </field>

            <!-- RXFIR Field -->
            <field>
               <name>RXFIR</name>
