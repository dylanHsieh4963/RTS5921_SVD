                        </enumeratedValue>
                        <enumeratedValue>
                            <name>FRAME_05BITS</name>
                            <description>5-bit serial data transfer</description>
                            <value>0x4</value>
                        </enumeratedValue>
                        <enumeratedValue>
                            <name>FRAME_06BITS</name>
                            <description>6-bit serial data transfer</description>
                            <value>0x5</value>
                        </enumeratedValue>
                        <enumeratedValue>
                            <name>FRAME_07BITS</name>
                            <description>7-bit serial data transfer</description>
                            <value>0x6</value>
                        </enumeratedValue>
                        <enumeratedValue>
                            <name>FRAME_08BITS</name>
                            <description>8-bit serial data transfer</description>
