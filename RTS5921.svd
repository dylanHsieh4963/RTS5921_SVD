      <headerStructName>SPIC</headerStructName>
      <baseAddress>0x40000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <!-- Address Range -->
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <!-- Interrup -->
      <interrupt>
        <name>ITR_SPIC</name>
        <description>Internal SPIC Interrupt</description>
        <value>9</value>
