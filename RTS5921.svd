                        <enumeratedValue>
                            <name>FRAME_09BITS</name>
                            <description>9-bit serial data transfer</description>
                            <value>0x8</value>
                        </enumeratedValue>
                        <enumeratedValue>
                            <name>FRAME_10BITS</name>
                            <description>10-bit serial data transfer</description>
